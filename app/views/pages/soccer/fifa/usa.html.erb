<!DOCTYPE html>
<html>
<head>

  <!-- renders google ads to the site -->
  <%= render 'shared/google_ads' %>

  <!-- adds google analytics to the site -->
  <%= render 'shared/google_analytics' %>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  
  <title>Backcourt Dribble</title>
</head>
<body>
  <!-- Begin Content -->
  <div class="container-fluid">

        <%= render 'shared/nav' %>


    <!-- Start Row 2 -->
    <div class="row">
      <!-- League menu panel -->
      <div class="col-md-3 league_menu menu">
        <!-- <p> Check out the WoSo's Wall! </p> -->

        <%= render "shared/fifa_menu" %>

        <!-- Ads within league menu panel-->
        <div class="ad-container">
          <div class="ad"></div>
          <div class="ad"></div>
          <div class="ad"></div>
          <div class="ad"></div>
        </div>
      </div> <!-- league menu panel -->

      <!-- Large twitter list panel -->
      <div class="col-md-5 large_twitter_list">
